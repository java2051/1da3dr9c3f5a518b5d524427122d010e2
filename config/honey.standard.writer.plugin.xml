<?xml version="1.0" encoding="UTF-8"?>
<honey-configuration version="v3.0">
	<description>
		honey.standard.writer.plugin插件的配置
	</description>
	
	<plugin id="honey.standard.writer.plugin"
		storage="CHECKING"
		charset ="utf8" 
		enableWrite="false"
	/>
	<!-- 
		storage 存储类型 其值为: backup(备份存储) multi(多文件存储) override(覆盖存储)
		charset和 encoding 相同都是文件字符集
		enableWrite 存储文件是否可写入 true:是 false:否 默认false
	-->
	
</honey-configuration>
