<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="honey.configuration.plugin" version="0.0.1"
	 class="com.honey.configureation.ConfigurationPlugin">
	
	 
	<runtime>
		<library id="core" path="classes/" type="code">
			<doc caption="API documentation">
				<doc-ref path="api/index.html" caption="javadoc" />
			</doc>
			<export prefix="*" />
		</library>
	</runtime>
	<extension-point id="ConfigurationHolder">
		<parameter-def id="isHolder" multiplicity="none-or-one"/>
	</extension-point>
		 
</plugin>